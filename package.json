{
  "name": "collaborative-code-editor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter example dev",
    "build": "pnpm --filter @collaborative-editor/core build && pnpm --filter example build",
    "start": "pnpm --filter example start",
    "lint": "pnpm --filter example lint",
    "ws": "pnpm --filter @collaborative-editor/server start",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm ws\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
